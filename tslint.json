{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "array-type": false,
    "forin": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "no-console": false,
    "prefer-for-of": false,
    "variable-name": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}
